                <!-- Select Highlights Section -->
                <div class="col-lg-3 col-md-6 col-sm-12 mb-4">
                    <h6 class="fw-bold mb-3">SELECT HIGHLIGHTS</h6>
                    <ul class="list-unstyled">
                        <li><a href="#" style="color: #ffffff; text-decoration: none;">OEM Network Solutions</a></li>
                        <li><a href="#" style="color: #ffffff; text-decoration: none;">ITAD-E-Waste</a></li>
                        <li><a href="#" style="color: #ffffff; text-decoration: none;">Fiber to the Home Repair</a></li>
                        <li><a href="#" style="color: #ffffff; text-decoration: none;">Fastback Networks Repair Services</a></li>
                        <li><a href="#" style="color: #ffffff; text-decoration: none;">Careers</a></li>
                        <li><a href="#" style="color: #ffffff; text-decoration: none;">Executive Management Team</a></li>
                        <li><a href="#" style="color: #ffffff; text-decoration: none;">SFP Transceivers</a></li>
                        <li><a href="#" style="color: #ffffff; text-decoration: none;">Site Monitoring</a></li>
                    </ul>
                </div>

                <!-- Network Solutions Section -->
                <div class="col-lg-3 col-md-6 col-sm-12 mb-4">
                    <h6 class="fw-bold mb-3">NETWORK SOLUTIONS</h6>
                    <ul class="list-unstyled">
                        <li><a href="#" style="color: #ffffff; text-decoration: none;">Multi-Vendor Network Equipment</a></li>
                        <li><a href="#" style="color: #ffffff; text-decoration: none;">Multi-Vendor Network Equipment Repair</a></li>
                        <li><a href="#" style="color: #ffffff; text-decoration: none;">Asset Management & Logistics</a></li>
                        <li><a href="#" style="color: #ffffff; text-decoration: none;">Remote Technical Services</a></li>
                        <li><a href="#" style="color: #ffffff; text-decoration: none;">Network Test & Visibility</a></li>
